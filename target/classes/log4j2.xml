<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
     
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d [%-6p] %c{1} - %m%n"/>
    </Console>   
                
  </Appenders>
 
   
     
  <Loggers>
    <Root level="all">
      <AppenderRef ref="Console"/>
      <AppenderRef ref="logfile" level="info"/>
    </Root>
  </Loggers>
</Configuration>